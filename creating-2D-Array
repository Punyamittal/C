/*creating a 2D array*/
#include <stdio.h>
void main() {
    int a,b;
    printf("enter the value for row:");
    scanf("%d",&a);
    printf("\n");
    printf("enter the value for coloumn:");
    scanf("%d",&b);
    printf("\n");
    int mat[a][b];
    for(int i=0;i<a;i++){
        for(int j=0;j<b;j++){
            int d;
            printf("enter value for position[%d][%d]:",i+1,j+1);
            scanf("%d", &mat[i][j]);
        }
    }
    printf("the 2D vector is:\n");
    for(int i=0;i<2;i++){
        for(int j=0;j<3;j++){
            printf("%d",mat[i][j]);
            printf(" ");
        }
        printf("\n");
    }
}
