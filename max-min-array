#include <stdio.h>
int main() {
    printf("enter the number of elements:");
    int a;
    scanf("%d",&a);
    printf("\n");
    int arr[a];
    for(int i = 0;i<a;i++){
        printf("enter the element :");
        scanf("%d",&arr[i]);
        printf("\n");
    }
    int n=0;
    for(int i = 0;i<a;i++){
        if(n<("%d",arr[i])){
            n=("%d",arr[i]);
        }
    }
    printf("the maximum number is:");
    printf("%d",n);
    int m=0;
    for(int i = 0;i<a;i++){
        if(n>("%d",arr[i])){
            n=("%d",arr[i]);
        }
    }
    printf("\n");
    printf("the minimum number is:");
    printf("%d",n);
    return 0;
}
